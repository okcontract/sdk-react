{
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules/**/*",
    "src/*.test.ts"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "typeRoots": [
      "node_modules/@types"
    ],
    "declaration": true,
    "emitDeclarationOnly": true,
    "outDir": "def",
    "moduleResolution": "Bundler",
    "module": "ES2020",
    "target": "ES2020",
    "skipLibCheck": true,
    "noEmitOnError": false,
    "jsx": "react-jsx"
  }
}